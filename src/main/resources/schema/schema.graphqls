type Query {
    shows(titleFilter: TitleFilter): [Show]
    withContext: String
}

type Mutation {
    addRating(input: RatingInput): Rating
}

type Show {
    title: String
    releaseYear: Int
    actors: [Actor]
}

type Actor {
    name: String
}

input TitleFilter {
    name: String
}

type Rating {
    avgStars: Float
}

input RatingInput {
    title: String
    stars: Int
}